{
    "auth": {
        "frontend": {
            "customAuth": false,
            "dependsOn": [],
            "frontendAuthConfig": {
                "mfaConfiguration": "OPTIONAL",
                "mfaTypes": ["TOTP"],
                "passwordProtectionSettings": {
                    "passwordPolicyCharacters": [
                        "REQUIRES_LOWERCASE",
                        "REQUIRES_NUMBERS",
                        "REQUIRES_SYMBOLS",
                        "REQUIRES_UPPERCASE"
                    ],
                    "passwordPolicyMinLength": 8
                },
                "signupAttributes": ["NAME"],
                "socialProviders": [],
                "usernameAttributes": [],
                "verificationMechanisms": ["EMAIL"]
            },
            "providerPlugin": "awscloudformation",
            "service": "Cognito"
        }
    }
}
